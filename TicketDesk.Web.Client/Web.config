		<!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
		<section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
	</configSections>
	<connectionStrings>
		<add name="TicketDesk" connectionString="data source=(LocalDb)\v11.0;Integrated Security=SSPI;AttachDBFilename=|DataDirectory|TicketDesk.mdf" providerName="System.Data.SqlClient" />
	</connectionStrings>
	<appSettings>
		<!-- SetupEnabled allows ticketdesk to run the setup wizard on startup if/when it detects
					that no database exists, or that the database is a legacy TD 2.1x database. Once 
					you complete the setup once, you should disable this setting here to prevent the
					wizard from being run again in the future -->
		<add key="ticketdesk:SetupEnabled" value="false" />
		<!-- DemoModeEnabled causes ticketdesk to automatically create demo users and demo ticket 
					data, when disabled only the default admin user and required default settings will 
					be created -->
		<add key="ticketdesk:DemoModeEnabled" value="true" />
		<!-- ResetDemoDataOnStartup causes ticketdesk to remove all data and recreate demo data
					each time the applicaiton restarts. This setting is used in hosted online demos -->
		<add key="ticketdesk:ResetDemoDataOnStartup" value="false" />
		<add key="webpages:Version" value="3.0.0.0" />
		<add key="webpages:Enabled" value="false" />
		<add key="ClientValidationEnabled" value="true" />
		<add key="UnobtrusiveJavaScriptEnabled" value="true" />

	</appSettings>
	<system.web>
		<customErrors mode="Off"></customErrors>
		<compilation debug="true" targetFramework="4.5.1" />
